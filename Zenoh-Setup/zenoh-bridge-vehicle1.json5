{
    "plugins": {
    "ros2dds": {
      "nodename": "zenoh_vehicle2_bridge",
      "namespace": "/vehicle1",
      
      // Set publish priorities to reduce latency for critical topics
      "pub_priorities": [
        "/clock=1:express",
        "/sensing/lidar/top/pointcloud_raw=2",
        "/sensing/lidar/top/pointcloud_raw_ex=2",
        "/sensing/imu/tamagawa/imu_raw=3",
        "/sensing/gnss/pose=3",
        "/sensing/gnss/pose_with_covariance=3",
        "/vehicle/status/velocity_status=4"
      ],
      deny: {
      subscribers: ["/sensing/camera/traffic_light/camera_info", "/vehicle2/sensing/camera/traffic_light/image_raw"],
      publishers: ["/sensing/camera/traffic_light/camera_info", "/vehicle2/sensing/camera/traffic_light/image_raw"], 
      },
    },
  },
  transport: {
    link: {
      tx: {
        queue:{
          /// Perform batching of messages if they are smaller of the batch_size
          batching: {
            /// Default works well in most cases. If performance problems arise, remove the comment to disable batching.
            // enabled: false,
          }
        }
      }
    }
  }
} 
