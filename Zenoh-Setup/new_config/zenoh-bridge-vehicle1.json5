{
  "mode": "peer",
  "id": "vehicle1-client",
  "connect": ["tcp/10.0.0.91:7447"],  // ‚Üê replace with actual IP
  "plugins": {
    "ros2dds": {
      "nodename": "zenoh_vehicle1_bridge",
      "namespace": "/vehicle1"
    }
  },
  transport: {
    link: {
      tx: {
        queue:{
