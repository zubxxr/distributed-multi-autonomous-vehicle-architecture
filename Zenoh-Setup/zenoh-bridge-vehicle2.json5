{
  "plugins": {
    "ros2dds": {
      "nodename": "zenoh_vehicle2_bridge",
      "namespace": "/vehicle2",
        "pub_priorities": [
          "/initialpose=1:express",
          "/tf=1:express",
          "/tf_static=1:express",
          "/clock=1:express",
          "/localization/.*=2",
          "/sensing/.*=3",
          "/avp/vehicle_count=4",
          "/avp/parking_spots=4",
          "/avp/queue=4",
          "/avp/reserved_parking_spots=4",
          "/rosout=7"
        ]
    }
  },
  "transport": {
    "link": {
      "tx": {
        "queue": {
          "batching": {}
        }
      }
    }
  },
  scouting: {
    multicast: {
      enabled: false
    },
    gossip: {
      enabled: false
    }
  }
}
